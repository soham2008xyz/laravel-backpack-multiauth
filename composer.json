{
    "name": "zoutapps/laravel-backpack-multiauth",
    "description": "This package enables using different Guards and User Models for laravel-backpack.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Ziegler",
            "email": "oliver.ziegler@zoutapps.de"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php" : ">=7.0",
        "illuminate/auth": "~5.5.0",
        "illuminate/container": "~5.5.0",
        "illuminate/contracts": "~5.5.0",
        "illuminate/database": "~5.5.0",
        "illuminate/filesystem": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "laravel/laravel": "^5.5",
        "backpack/base": "^0.8.4"
    },
    "autoload": {
        "psr-4": {
            "ZoutApps\\LaravelBackpackAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZoutApps\\LaravelBackpackAuth\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZoutApps\\LaravelBackpackAuth\\AuthServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
